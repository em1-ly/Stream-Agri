{
  "cli": {
    "version": ">= 16.4.0",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "android": {
        "buildType": "apk"
      },
      "channel": "development"
    },
    "simulator": {
      "distribution": "internal",
      "ios": {
        "simulator": true
      },
      "channel": "simulator"
    },
    "preview": {
      "distribution": "internal",
      "channel": "preview"
    },
    "ad-hoc": {
      "distribution": "internal",
      "ios": {
        "buildConfiguration": "Release"
      },
      "channel": "preview"
    },
    "ad-hoc-debug": {
      "distribution": "internal",
      "ios": {
        "buildConfiguration": "Debug"
      },
      "channel": "preview",
      "env": {
        "EXPO_NO_DEV_CLIENT": "1"
      }
    },
    "production": {
      "autoIncrement": true,
      "distribution": "store",
      "ios": {
        "buildConfiguration": "Release"
      },
      "android": {
        "buildType": "apk"
      },
      "channel": "production",
      "env": {
        "EXPO_NO_DEV_CLIENT": "1"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
